<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>aottg_tournament_bot</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AottgBotLib" Version="1.0.0"/>
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Photon-NetStandard">
      <HintPath>..\Dependencies\Photon-NetStandard.dll</HintPath>
    </Reference>
    <Reference Include="PhotonRealtime-NetStandard">
      <HintPath>..\Dependencies\PhotonRealtime-NetStandard.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>